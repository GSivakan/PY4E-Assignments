CREATE TABLE Ages ( 
  name VARCHAR(128), 
  age INTEGER
)

DELETE FROM Ages;
INSERT INTO Ages (name, age) VALUES ('Koni', 18);
INSERT INTO Ages (name, age) VALUES ('Laird', 18);
INSERT INTO Ages (name, age) VALUES ('Griffyn', 25);
INSERT INTO Ages (name, age) VALUES ('Maxwell', 14);



SELECT hex(name || age) AS X FROM Ages ORDER BY X
